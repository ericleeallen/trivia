<h1>Listing questions</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Question</th>
    <th>Answer</th>
    <th>Difficulty</th>
    <th>Correct Answer</th>
    <th></th>
    <th></th>
  </tr>

<% @questions.each do |question| %>
  <tr>
    <td><%= question.id %></td>
    <td><%= question.question %></td>
    <td><%= question.answer %></td>
    <td><%= question.difficulty %></td>
    <td><%= question.correctanswer %></td>
    
  </tr>
<% end %>
</table>

