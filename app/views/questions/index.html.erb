<h1>Listing questions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Question</th>
    <th>Difficulty</th>
    <th>Date</th>
    <th></th>
    <th></th>
  </tr>

<% @questions.each do |question| %>
  <tr>
    <td><%= question.question %></td>
    <td><%= question.answer %></td>
    <td><%= question.difficulty %></td>
    <td><%= question.correctanswer %></td>
    
  </tr>
<% end %>
</table>

